{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"strict": true,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"forceConsistentCasingInFileNames": true,

		"checkJs": true,
		"sourceMap": true,
		"resolveJsonModule": true
	},
	"exclude": ["node_modules", "src/lib/**/*"]
}
