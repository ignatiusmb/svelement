{
  "name": "syv",
  "author": "Ignatius Bagus",
  "description": "Svelte Complementary Library",
  "version": "0.0.15",
  "license": "MIT",
  "type": "module",
  "svelte": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "reset": "git reset --hard && git clean -dfx -e node_modules",
    "dev": "pnpm build && vite dev",
    "sync": "svelte-kit sync",
    "build": "pnpm sync && pnpm build:icons",
    "build:icons": "cd src/lib/icons && node build.mjs",
    "package": "pnpm build && svelte-package",
    "format": "prettier --write .",
    "check": "pnpm check:style && pnpm check:types && pnpm check:svelte",
    "check:style": "prettier --check .",
    "check:types": "pnpm build && tsc --noEmit",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.json"
  },
  "packageManager": "pnpm@7.13.5",
  "prettier": "mauss/prettier.json",
  "repository": "github:ignatiusmb/syv",
  "bugs": "https://github.com/ignatiusmb/syv/issues",
  "keywords": [
    "svelte",
    "complementary",
    "components",
    "icons",
    "loaders",
    "stores",
    "library",
    "kit"
  ],
  "dependencies": {
    "mauss": "^0.3.3"
  },
  "peerDependencies": {
    "svelte": "^3.49.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "latest",
    "@types/feather-icons": "^4.7.0",
    "@types/prettier": "^2.7.1",
    "feather-icons": "^4.29.0",
    "marqua": "^0.3.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.19",
    "typescript": "^4.8.4",
    "vite": "^3.1.4"
  }
}
