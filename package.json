{
  "name": "syv",
  "version": "0.0.27",
  "description": "Svelte Complementary Library",
  "repository": "github:ignatiusmb/syv",
  "author": "Ignatius Bagus",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "reset": "git reset --hard && git clean -dfx -e node_modules",
    "dev": "pnpm prep && vite dev",
    "build": "pnpm prep && vite build",
    "format": "prettier --write . --plugin-search-dir=.",
    "check": "pnpm prep && pnpm check:style && pnpm check:svelte",
    "check:style": "prettier --check . --plugin-search-dir=.",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.json",
    "package": "pnpm prep && svelte-package && pnpm copy",
    "prep": "pnpm prep:icons && svelte-kit sync",
    "prep:icons": "cd src/lib/icons && node build.mjs",
    "copy": "cp package.json README.md CHANGELOG.md LICENSE dist/"
  },
  "svelte": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": "./index.js",
    "./core": "./core/index.js",
    "./core/*.svelte": "./core/*.svelte",
    "./action": "./action/index.js",
    "./action/click": "./action/click.js",
    "./icons/*.svelte": "./icons/*.svelte",
    "./icons/feather/*": "./icons/feather/*.js",
    "./internal/*.svelte": "./internal/*.svelte",
    "./loader/*.svelte": "./loader/*.svelte",
    "./store": "./store/index.js",
    "./store/api": "./store/api.js",
    "./store/browser": "./store/browser.js",
    "./options": "./options.js",
    "./types": "./types.js",
    "./utils": "./utils.js"
  },
  "packageManager": "pnpm@8.6.4",
  "prettier": "mauss/prettier.json",
  "keywords": [
    "svelte",
    "complementary",
    "components",
    "icons",
    "loaders",
    "stores",
    "library",
    "kit"
  ],
  "dependencies": {
    "mauss": "^0.6.0"
  },
  "peerDependencies": {
    "svelte": "^3.49.0 || ^4.0.0"
  },
  "devDependencies": {
    "@ignatiusmb/styles": "^0.1.1",
    "@sveltejs/adapter-vercel": "^3.0.2",
    "@sveltejs/kit": "^1.22.3",
    "@sveltejs/package": "^2.2.0",
    "@types/feather-icons": "^4.29.1",
    "@types/prettier": "^2.7.3",
    "feather-icons": "^4.29.0",
    "marqua": "^0.5.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "svelte2tsx": "^0.6.19",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  }
}
