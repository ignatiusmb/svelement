{
  "name": "syv",
  "author": "Ignatius Bagus",
  "description": "Svelte Complementary Library",
  "version": "0.0.6",
  "license": "MIT",
  "type": "module",
  "svelte": "index.js",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "npm run build && svelte-kit dev",
    "check": "prettier -c \"src/{lib,routes}/**/*\" --ignore-path src/lib/.gitignore",
    "format": "prettier -w \"src/{lib,routes}/**/*\" --ignore-path src/lib/.gitignore",
    "build": "run-p build:**",
    "build:icons": "cd src/lib/icons && node build.mjs",
    "package": "npm run build && svelte-kit package",
    "reset": "git reset --hard && git clean -dfx -e node_modules"
  },
  "prettier": "mauss/prettier.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/ignatiusmb/syv.git"
  },
  "bugs": {
    "url": "https://github.com/ignatiusmb/syv/issues"
  },
  "keywords": [
    "svelte",
    "complementary",
    "components",
    "icons",
    "loaders",
    "stores",
    "library",
    "kit"
  ],
  "dependencies": {
    "mauss": "^0.1.2"
  },
  "peerDependencies": {
    "svelte": "^3.31.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "^10.3.4",
    "cssnano": "^5.0.8",
    "feather-icons": "^4.28.0",
    "markdown-it": "^12.0.6",
    "marqua": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.0",
    "svelte": "^3.31.2",
    "svelte-preprocess": "^4.7.3",
    "svelte2tsx": "^0.4.5",
    "tailwindcss": "^2.2.9",
    "typescript": "^4.2.4"
  }
}
