{
	"name": "syv",
	"version": "0.0.30",
	"description": "Svelte Complementary Library",
	"repository": "github:ignatiusmb/syv",
	"author": "Ignatius Bagus",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"reset": "git reset --hard && git clean -dfx -e node_modules",
		"dev": "pnpm prep && vite dev",
		"build": "pnpm prep && vite build",
		"format": "prettier -w . --plugin=prettier-plugin-svelte --plugin=prettier-plugin-sort-package-json",
		"check": "pnpm prep && pnpm check:style && pnpm check:svelte",
		"check:style": "prettier -c . --plugin=prettier-plugin-svelte --plugin=prettier-plugin-sort-package-json",
		"check:svelte": "svelte-check --tsconfig ./tsconfig.json",
		"package": "pnpm prep && svelte-package && pnpm copy",
		"prep": "pnpm prep:icons && svelte-kit sync",
		"prep:icons": "cd src/lib/icons && node build.mjs",
		"copy": "cp package.json README.md LICENSE dist/"
	},
	"module": "./index.js",
	"svelte": "./index.js",
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./core": "./core/index.js",
		"./core/*.svelte": "./core/*.svelte",
		"./action": "./action/index.js",
		"./action/click": "./action/click.js",
		"./icons/*.svelte": "./icons/*.svelte",
		"./icons/*": "./icons/*.js",
		"./internal/*.svelte": "./internal/*.svelte",
		"./loader/*.svelte": "./loader/*.svelte",
		"./store": "./store/index.js",
		"./store/api": "./store/api.js",
		"./store/browser": "./store/browser.js",
		"./options": "./options.js",
		"./types": "./types.js",
		"./utils": "./utils.js"
	},
	"packageManager": "pnpm@8.12.1",
	"prettier": "mauss/prettier.json",
	"keywords": [
		"svelte",
		"complementary",
		"components",
		"icons",
		"loaders",
		"stores",
		"library",
		"kit"
	],
	"dependencies": {
		"mauss": "^0.7.2"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@ignatiusmb/styles": "^0.1.1",
		"@sveltejs/adapter-vercel": "^4.0.2",
		"@sveltejs/kit": "^2.0.2",
		"@sveltejs/package": "^2.2.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@types/feather-icons": "^4.29.4",
		"feather-icons": "^4.29.1",
		"marqua": "^0.5.6",
		"prettier": "^3.1.1",
		"prettier-plugin-sort-package-json": "^0.2.0",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "next",
		"svelte-check": "^3.6.2",
		"svelte2tsx": "^0.6.27",
		"typescript": "^5.3.3",
		"vite": "^5.0.10"
	}
}
