{
  "name": "syv",
  "author": "Ignatius Bagus",
  "description": "Svelte Complementary Library",
  "version": "0.0.25",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "pnpm build && vite dev",
    "format": "prettier --write . --plugin-search-dir=.",
    "check": "pnpm build && pnpm check:style && pnpm check:svelte",
    "check:style": "prettier --check . --plugin-search-dir=.",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.json",
    "reset": "git reset --hard && git clean -dfx -e node_modules",
    "package": "pnpm build && svelte-package && pnpm copy",
    "build": "svelte-kit sync && pnpm build:icons",
    "build:icons": "cd src/lib/icons && node build.mjs",
    "copy": "pnpm copy:pkg && pnpm copy:rdm && pnpm copy:cgl && pnpm copy:lcs",
    "copy:pkg": "cp package.json dist/",
    "copy:rdm": "cp README.md dist/",
    "copy:cgl": "cp CHANGELOG.md dist/",
    "copy:lcs": "cp LICENSE dist/"
  },
  "types": "./index.d.ts",
  "svelte": "./index.js",
  "module": "./index.js",
  "exports": {
    ".": "./index.js",
    "./core": "./core/index.js",
    "./core/*.svelte": "./core/*.svelte",
    "./action": "./action/index.js",
    "./action/click": "./action/click.js",
    "./icons/*.svelte": "./icons/*.svelte",
    "./icons/feather/*": "./icons/feather/*.js",
    "./internal/*.svelte": "./internal/*.svelte",
    "./loader/*.svelte": "./loader/*.svelte",
    "./store": "./store/index.js",
    "./store/api": "./store/api.js",
    "./store/browser": "./store/browser.js",
    "./options": "./options.js",
    "./types": "./types.js",
    "./utils": "./utils.js"
  },
  "packageManager": "pnpm@8.3.1",
  "prettier": "mauss/prettier.json",
  "repository": "github:ignatiusmb/syv",
  "bugs": "https://github.com/ignatiusmb/syv/issues",
  "keywords": [
    "svelte",
    "complementary",
    "components",
    "icons",
    "loaders",
    "stores",
    "library",
    "kit"
  ],
  "dependencies": {
    "mauss": "^0.4.11"
  },
  "peerDependencies": {
    "svelte": "^3.49.0"
  },
  "devDependencies": {
    "@ignatiusmb/styles": "^0.1.1",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.15.7",
    "@sveltejs/package": "^2.0.2",
    "@types/feather-icons": "^4.29.1",
    "@types/prettier": "^2.7.2",
    "feather-icons": "^4.29.0",
    "marqua": "^0.4.4",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte2tsx": "^0.6.11",
    "typescript": "^5.0.4",
    "vite": "^4.3.1"
  }
}
