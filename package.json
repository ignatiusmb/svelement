{
  "name": "syv",
  "author": "Ignatius Bagus",
  "description": "Svelte Complementary Library",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "svelte": "index.js",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "svelte-kit dev",
    "check": "prettier -c src",
    "format": "prettier -w src",
    "package": "svelte-kit package && run-p package:**",
    "package:icons": "node package/icons/build.mjs",
    "reset": "git reset --hard && git clean -dfx -e node_modules"
  },
  "exports": {
    ".": "./index.js",
    "./icons": {
      "import": "./icons/index.js"
    },
    "./loader": {
      "import": "./loader/index.js"
    },
    "./store": {
      "import": "./store/index.js"
    },
    "./package.json": "./package.json"
  },
  "prettier": "mauss/prettier.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/ignatiusmb/syv.git"
  },
  "bugs": {
    "url": "https://github.com/ignatiusmb/syv/issues"
  },
  "keywords": [
    "svelte",
    "complementary",
    "components",
    "icons",
    "loaders",
    "stores",
    "library",
    "kit"
  ],
  "dependencies": {
    "mauss": "^0.1.2"
  },
  "peerDependencies": {
    "svelte": "^3.31.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "^1.0.0-next.113",
    "feather-icons": "^4.28.0",
    "markdown-it": "^12.0.6",
    "marqua": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.0",
    "svelte": "^3.31.2",
    "svelte-preprocess": "^4.7.3",
    "typescript": "^4.2.4"
  }
}
