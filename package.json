{
  "name": "svelement",
  "author": "Ignatius Bagus",
  "description": "Reusable Svelte components",
  "version": "0.0.6",
  "license": "MIT",
  "svelte": "index.js",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "check": "prettier -c src",
    "format": "prettier -w src",
    "build": "run-p build:**",
    "build:icons": "node src/icons/build.js",
    "prepublishOnly": "npm run build && mv src/* ."
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./index.js",
    "./icons": "./icons/index.js",
    "./loader": "./loader/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ignatiusmb/svelement.git"
  },
  "bugs": {
    "url": "https://github.com/ignatiusmb/svelement/issues"
  },
  "keywords": [
    "svelte",
    "reusable",
    "elements",
    "components",
    "styles"
  ],
  "dependencies": {
    "mauss": "0.0.1"
  },
  "devDependencies": {
    "feather-icons": "^4.28.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.1.1",
    "svelte": "^3.31.0",
    "svelte-preprocess": "^4.6.1"
  }
}
