{
  "name": "svelement",
  "author": "Ignatius Bagus",
  "description": "Reusable Svelte components",
  "version": "0.0.10",
  "license": "MIT",
  "svelte": "src/index.js",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "check": "prettier -c src",
    "format": "prettier -w src",
    "build": "run-p build:**",
    "build:icons": "node src/icons/build.js",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.js",
    "./icons": "./src/icons/index.js",
    "./loader": "./src/loader/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ignatiusmb/svelement.git"
  },
  "bugs": {
    "url": "https://github.com/ignatiusmb/svelement/issues"
  },
  "keywords": [
    "svelte",
    "reusable",
    "elements",
    "components",
    "styles"
  ],
  "dependencies": {
    "mauss": "0.0.3"
  },
  "peerDependencies": {
    "svelte": "^3.31.0"
  },
  "devDependencies": {
    "feather-icons": "^4.28.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.1.1",
    "svelte": "^3.31.0",
    "svelte-preprocess": "^4.6.1"
  }
}
