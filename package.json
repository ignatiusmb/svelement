{
  "name": "svelement",
  "author": "Ignatius Bagus",
  "description": "Reusable Svelte components",
  "version": "0.0.13",
  "license": "MIT",
  "type": "module",
  "svelte": "index.js",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "check": "prettier -c src",
    "format": "prettier -w src",
    "build": "run-p build:**",
    "build:tsc": "tsc -d",
    "build:icons": "node src/icons/build.mjs",
    "prepublishOnly": "npm run build && mv src/* .",
    "reset": "git reset --hard && git clean -dfx -e node_modules"
  },
  "exports": {
    ".": "./index.js",
    "./icons": {
      "import": "./icons/index.js"
    },
    "./icons/feather": {
      "import": "./icons/feather/index.js"
    },
    "./loader": {
      "import": "./loader/index.js"
    },
    "./store": {
      "import": "./store/index.js"
    },
    "./package.json": "./package.json"
  },
  "prettier": "mauss/prettier.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/ignatiusmb/svelement.git"
  },
  "bugs": {
    "url": "https://github.com/ignatiusmb/svelement/issues"
  },
  "keywords": [
    "svelte",
    "reusable",
    "elements",
    "components",
    "styles"
  ],
  "dependencies": {
    "mauss": "^0.0.11"
  },
  "peerDependencies": {
    "svelte": "^3.31.0"
  },
  "devDependencies": {
    "feather-icons": "^4.28.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.0",
    "svelte": "^3.31.2",
    "svelte-preprocess": "^4.6.1",
    "typescript": "^4.2.4"
  }
}
